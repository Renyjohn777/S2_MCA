  
  
create database college;
  
  
  
use college;

create table student(rollno int(10),name varchar(30),dob date,place varchar(30),phno int(10));

create table course(course_code int(10),course_name Varchar(30),credit int(6));

show tables;

+-------------------+
| Tables_in_college |
+-------------------+
| course            |
| student           |
+-------------------+


